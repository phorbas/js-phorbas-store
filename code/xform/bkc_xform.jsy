import { bkc_storage_verify } from '../core.jsy'
import { bkc_multi } from './bkc_multi.jsy'

export * from '../core.jsy'

export async function bkc_with_xform(stg_tgt, opt={}) ::
  opt = @{} ...opt, stg_tgt: stg_tgt && await bkc_storage_verify(stg_tgt)
  return bkc_multi(opt)

export default bkc_with_xform

