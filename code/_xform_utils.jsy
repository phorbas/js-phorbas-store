export async function _batch_await_inline(batch) ::
  for let k in batch ::
    batch[k] = await batch[k]
  return batch

export function _bind_decode_key_batch(_xf_decode_key) ::
  if _xf_decode_key ::
    return batch => ::
      for let pair of batch ::
        pair[0] = _xf_decode_key(pair[0])
      return batch

