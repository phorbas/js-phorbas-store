import {validate_backend} from '@phorbas/store/esm/validate_backend.js'
import {bkc_with_fsp} from '@phorbas/store/esm/local/local_fsp.js'

if globalThis.process ::
  const fsp = import('node:fs/promises')
  validate_backend @ 'bkc_with_fsp', ()=> bkc_with_fsp({fsp})
  validate_backend @ 'bkc_with_fsp immutable', ()=> bkc_with_fsp({fsp, immutable: true})
