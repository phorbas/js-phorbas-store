{
  "private": true,
  "dependencies": {
    "aws4fetch": "^1.0.13",
    "ky": "^0.28.5",
    "level-mem": "^5.0.1",
    "mocha": "^9.1.1",
    "node-fetch": "^2.6.2"
  },
  "devDependencies": {
    "@phorbas/opaque": "file:../../opaque",
    "@phorbas/store": "file:..",
    "qsrv": "^0.1.9"
  },
  "scripts": {
    "prebuild": "test -d ./node_modules || npm -s install",
    "build": "../node_modules/.bin/rollup --config",
    "clean": "rm ./__unittest.*",
    "watch": "npm -s run build -- --watch",
    "dkr_deps": "cd int--core; npm -s run dkr_deps",
    "dkr_prune": "docker stack deploy --prune -c int--core/deps-deploy.yml integ_phorbas_store",
    "preserve": "npm -s run build ; npm -s run dkr_deps",
    "serve": "npm -s run serve:one",
    "serve:one": "qsrv -f unittest.html -- *.html __unittest.iife.js",
    "pretest": "npm -s run build",
    "test": "npm -s run test:one",
    "test:one": "NODE_OPTIONS=--enable-source-maps  mocha ./__unittest.node.cjs",
    "test:min": "NODE_OPTIONS=--enable-source-maps  mocha --reporter=min ./__unittest.node.cjs"
  }
}
